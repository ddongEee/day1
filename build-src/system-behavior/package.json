{
  "name": "system-behavior",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "test:console": "cucumber-js --tags \"not @WIP\" -p console",
    "test": "yarn run test:report && yarn run report",
    "test:report": "cucumber-js --tags \"not @WIP\" -p report",
    "report": "yarn run report:remove && yarn run report:create",
    "report:remove": "rm -rf outputs/report",
    "report:create": "ts-node cucumber.report.ts --module commonjs",
    "lint": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.9.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@types/chai": "^4.3.4",
    "@types/cucumber": "^7.0.0",
    "@types/node": "^18.11.10",
    "@yarnpkg/sdks": "^3.0.0-rc.32",
    "chai": "^4.3.7",
    "cucumber-tsflow": "^4.0.0-rc.7",
    "lint-staged": "^13.1.0",
    "loglevel": "^1.8.1",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "pactum": "^3.3.1",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
